    public void SetAjusta(String sku, String nombre, String medida, String valor_unitario,String cantidad,String recibido, String pendiente, String valor_actual){

        Sku = sku;
        Nombre = nombre;
        Medida = medida;
        Unitario = Double.parseDouble(valor_unitario);
        Cantidad = Double.parseDouble(cantidad);
        txRecibido = recibido.toString().replaceAll(",", "");
        Recibido = Double.parseDouble(txRecibido);
//      Recibido = Double.parseDouble(recibido);
        Pendiente = pendiente;
        txPrecioActual.setText(valor_actual);
        valor = txPrecioActual.getText().replaceAll(",","");
        valora = Double.parseDouble(valor);
                       
    }
    
    
