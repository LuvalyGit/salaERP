    private void btAjustarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btAjustarActionPerformed
        
        double diferencia = 0;
        if (txPrecioAjustado.getText().equals("")){
        
             txPrecioAjustado.setText("0");
        
        }
        
        valorajusta = Double.parseDouble(txPrecioAjustado.getText());
        valora = valorajusta;
        
        Unitario2 = valora / Recibido;
        
//        System.out.println("EL UNITARIO 2 ES "+Unitario2);
        
        
        diferencia = Unitario - Unitario2;
        
        
//        System.out.println("LA DIFERENCIA ES "+diferencia);
        
//        if (diferencia > 2 || diferencia < -2){
//           
//            fmMain.Mensaje("Solicitar ajuste a Compras");
//            return;
//       
//        }  
        
        valor = String.valueOf(valora);
        unit2 = String.valueOf(Unitario2);
        unit = String.valueOf(Unitario);
        dif = String.valueOf(diferencia);
        
        Retorno = true;
      
        dispose();
       
        
    }//GEN-LAST:event_btAjustarActionPerformed
    
    
