		<queryString language="SQL">
			<![CDATA[select pd.sku,p.nombre, pu.um, pd.cantidad, pd.valorunitario,pd.totallinea, cc.idch
from ctacteprvdet pd
left join producto   p  on pd.sku=p.sku
left join par_unidad pu on p.unidad =pu.codigo 
 left join codchile cc on cc.sku = p.sku
where pd.nrodocto=2]]>
